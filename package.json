    "test": "gulp",
    "test:node": "npm run build && nyc --reporter=html --reporter=text mocha --timeout=3000 test/*.spec.js",
    "test:browser": "npm run build && karma start karma.conf.js",
    "tslint": "tslint --project tsconfig.json --config tslint.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCCDex/jcc_moac_utils.git"
  },
  "keywords": [
    "rpc",
    "api",
    "jcc",
    "jccdex",
    "decentralized",
    "moac"
  ],
  "author": "JCCDex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JCCDex/jcc_moac_utils/issues"
  },
  "homepage": "https://github.com/JCCDex/jcc_moac_utils#readme",
  "dependencies": {
    "bignumber.js": "^8.1.1",
    "chain3": "^0.1.18"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "gulp": "^4.0.1",
    "gulp-shell": "^0.7.0",
    "jsdom": "^15.0.0",
    "karma": "^4.1.0",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  },
  "sideEffects": [
    "./lib/**/*.js"
  ]
